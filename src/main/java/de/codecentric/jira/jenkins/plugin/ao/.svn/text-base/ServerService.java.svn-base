package de.codecentric.jira.hudson.plugin.ao;

import com.atlassian.activeobjects.tx.Transactional;

import java.util.List;

/**
 * Defines methods to work with a Server entity.
 */
@Transactional
public interface ServerService {
	Server add(String name, String url);
	
	void del(String name);
	
	Server find(String name);
	 
	List<Server> all();
}
